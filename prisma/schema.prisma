// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Detection {
  id         Int      @id @default(autoincrement())
  objects    Object[]
  detectedAt String
  lat        Float
  lon        Float
  picture    String
}

model Object {
  id          Int       @id @default(autoincrement())
  type        String
  probability Float
  xMin        Float
  yMin        Float
  xMax        Float
  yMax        Float
  detection   Detection @relation(fields: [id], references: [id])
}
